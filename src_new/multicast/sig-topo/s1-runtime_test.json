{
    "target": "bmv2",
    "p4info": "build/fabric_tunnel.p4.p4info.txt",
    "bmv2_json": "build/fabric_tunnel.json",
    "table_entries": [
      {
        "table": "MyIngress.flow_classifier",
        "match": {
          "hdr.ipv4.dstAddr": "10.0.0.2"
        },
        "action_name": "MyIngress.assign_multicast",
        "action_params": {
          "multicast_group": 1
        }
      },
      {
        "table": "MyIngress.myTunnel_operate",
        "match": {
          "hdr.myTunnel.flow_id": 1
        },
        "action_name": "MyIngress.assign_multicast",
        "action_params": {
          "multicast_group": 1
        }
      }
    ],
    "multicast_group_entries" : [
      {
        "multicast_group_id" : 1,
        "replicas" : [
          {
            "egress_port" : 1,
            "instance" : 1
          },
          {
            "egress_port" : 2,
            "instance" : 1
          },
          {
            "egress_port" : 3,
            "instance" : 1
          },
          {
            "egress_port" : 4,
            "instance" : 1
          }
        ]
      }
    ]
  }
  